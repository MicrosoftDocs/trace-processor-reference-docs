    <Member MemberName="ExecutionRequiredPowerRequestTimeout">
      <MemberSignature Language="C#" Value="public Nullable&lt;TimeSpan&gt; ExecutionRequiredPowerRequestTimeout { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Nullable`1&lt;valuetype System.TimeSpan&gt; ExecutionRequiredPowerRequestTimeout" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Windows.EventTracing.Power.IPowerConfigurationSnapshot.ExecutionRequiredPowerRequestTimeout" />
